{
  "family": "davinci-code-game-task-definition",
  "executionRoleArn": "arn:aws:iam::561166824006:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "dvc-main",
      "image": "public.ecr.aws/v0g8b0z8/dvc-main:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    },
    {
      "name": "dvc-game",
      "image": "public.ecr.aws/v0g8b0z8/dvc-game:latest",
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
